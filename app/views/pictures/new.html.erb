<h2>Add a picture</h2>
<%= form_tag "/pictures", :id => "new" do %>
	<p>
		<label>Artist</label><br>
		<%= text_field_tag "artist" %>
	</p>
	<p>
		<label>Title</label><br>
		<%= text_field_tag "title" %>
	</p>
	<p>
		<label>URL</label><br>
		<%= text_field_tag "url" %>
	</p>
	<p>
		<label>Thumbnail URL</label><br>
		<%= text_field_tag "thumbnail_url" %>
	</p>
	<p>
		<input type="submit">
	</p>
<% end %>