<%= form_for @picture do |f| %>
	<p>
		<label>Artist</label><br>
		<%= f.text_field "artist" %>
	</p>
	<p>
		<label>Title</label><br>
		<%= f.text_field "title" %>
	</p>
	<p>
		<label>URL</label><br>
		<%= f.text_field "url" %>
	</p>
	<p>
		<label>Thumbnail URL</label><br>
		<%= f.text_field "thumbnail_url" %>
	</p>
	<p>
		<%= f.submit "Update" %>	
	</p>
<% end %>
<img src="<%= @picture.url %>" id="fullscreen">