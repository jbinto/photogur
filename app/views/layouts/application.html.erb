<!DOCTYPE html>
<html>
<head>
  <title>Photogur</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<h1><%= link_to 'Photogur', root_path %></h1>
<% if flash[:error] %>
	<section id="flash" class="error"><%= flash[:error] %></section>
<% elsif flash[:notice] %>
	<section id="flash" class="notice"><%= flash[:notice] %></section>
<% end %>

<%= yield %>

</body>
</html>
